      <tab id="facilidades" label="Facilidades" insertAfterMso="tickmarks" keytip="F"> 
        <group id="formatarCelulas" label="Papéis de Trabalho" autoScale="false" centerVertically="true">
          
          <button id="inserirLogotipo" label="Inserir Logotipo" size="large" image="logo" supertip="Clique para inserir o logotipo da empresa." keytip="1" onAction="inserir_logotipo" />

          <separator id="sep_operacoes_00" />
					
					<button id="novo_wp" label="Papel de Trabalho Padrão" size="large" imageMso="HeaderFooterInsert" supertip="Clique para criar um novo papel de trabalho padrão." onAction="NovoPapelPadrao" />					
					
					<separator id="sep_operacoes_01" />

					<button id="preparar_documento" label="Preparar Documento" size="normal" imageMso="CreateReportBlankReport" supertip="Clique para realizar os ajustes iniciais no papel de trabalho." onAction="PrepararTrabalho" />
					
					<button id="inserir_titulos" label="Inserir Titulos" size="normal" imageMso="ControlTitle" supertip="Selecione um grupo de células e clique aqui para inserir um título padrão." onAction="inserir_titulos" />
					
					<button id="mesclara_justificar" label="Mesclar/Justificar" size="normal" imageMso="ReviewCompareTwoVersions" supertip="Selcione um grupo de células e clique aqui para mesclar e justificar os textos." onAction="QuadroDeTexto" />

					<separator id="sep_operacoes_02" />

					<menu id="funcoes" label="Funções" size="normal" imageMso="QueryBuilder"  >

						<menuSeparator id="sep_operacoes_03" title="Operações com Números"/>

						<button id="divir_mil" label="Dividir números por mil" supertip="Divide uma ou mais células com números por mil." onAction="ExecutarNumeros_DivideMil" imageMso="DecimalsDecrease"/>

						<button id="divir_cem" label="Dividir números por cem" supertip="Divide uma ou mais células com números por cem." onAction="ExecutarNumeros_DivideCem" imageMso="DecimalsDecrease"/>
						
						<menuSeparator id="sep_operacoes_04"/>

						<button id="remove_decimais" label="Remover casas decimais" supertip="Remove todas as casas decimais de uma ou mais células que contenham números." onAction="ExecutarNumeros_RemoveDecimais" imageMso="DecimalsDecrease"/>

						<button id="arredondar_numero" label="Arredondar números" supertip="Arredonda uma ou mais células que contenham números para números inteiros." onAction="ExecutarNumeros_Arredondar" imageMso="DecimalsDecrease"/>						

						<menuSeparator id="sep_operacoes_05"/>

						<button	id="inserir_aleatorios" label="Inserir Números Aleatórios" supertip="Inserir números aleatórios no intervalo selecionado." onAction="inserir_numeros_aleatorios" imageMso="BulletListDefault"/>						
						
						<menuSeparator id="sep_operacoes_06" title="Operações com Informações"/>
						
						<button id="remover_imagens" label="Deletar todas as imagens da planilha" imageMso="ApplyImageBackgroundTile" supertip="Deletar todas as imagens da planilha de uma só vez, incluindo as que não estão visíveis." keytip="Y" onAction="excluir_todas_as_imagens"/>

						<menuSeparator id="sep_operacoes_99"/>
						
						<button	id="numeracao" label="Numerar/Filtrar" imageMso="SmartArtAddBullet" supertip="Informações o responsável pelo desenvolvimento do Facilitador de Auditoria." keytip="Y" onAction="InserirContagemFiltro"/>
						
						<button	id="remover_acentos" label="Caracteres Especiais" imageMso="Translate" supertip="Substitui todos os caracteres especiais pela sua letra básica. Exemplo: ã por a, ñ por n, etc." keytip="O" onAction="CaracteresEspeciais"/>
						
						<button id="texto_p_numeros" label="Texto para números" supertip="Converte os números armazenados como texto para o padrão numérico." onAction="Truque_converter_intervalo_numeros" keytip="C" imageMso="AcceptInvitation" />

						<button id="inverter_sinais" label="Inverter Sinais" supertip="Inverter o sinal dos números." onAction="Truque_inverter_sinal" imageMso="CalculateFull" />

						<button id="pontos_p_virgula" label="Pontos por vírgula" supertip="Trocar pontos por vírgulas. De 1,000.00 para 8.000,00" onAction="Truque_trocar_ponto_virgula" imageMso="CommaStyle" />

						<menuSeparator id="sep_operacoes_07" title="Operações com Textos"/>

						<button		id="todas_maiusculas"
									label="Todas Maiúsculas"
									supertip="Colocar todas as células em maiúsculas"
									onAction="caixaAlta"
									imageMso="FormControlLabel"/>

						<button		id="todas_minusculas"
									label="Todas Minúsculas"
									supertip="Colocar todas as células em minúsculas."
									onAction="caixaBaixa"
									imageMso="FormControlLabel"/>	
					
						<button	id="pri_maiusculas" label="Primeiras maiúsculas" supertip="Colocar todas com as primeiras letras de cada palavra em maiúsculas." onAction="pri_maiuscula" imageMso="FormControlLabel"/>

						<menuSeparator id="sep_operacoes_08" title="Testar Números de Documento"/>

						<button id="validar_cnpj" label="Validar lista de CNPJs" supertip="Verifica se uma lista de CNPJs é válida" onAction="VerificarCNPJemLista" imageMso="AppendOnly"/>

						<button id="validar_cpf" label="Validar lista de CPFs" supertip="Verifica se uma lista de CPFs é válida" onAction="VerificarCPFemLista" imageMso="AppendOnly"/>

					</menu>

					<!-- GRUPO DE FUNÇÕES ESPECIAIS -->
					<menu id="arquivos" label="Arquivos" size="normal" imageMso="QueryBuilder"  >
						
						<menuSeparator id="sep_operacoes_09" title="Estrutura de Diretórios"/>

						<button	id="criar_estrutura" label="Criar Diretório de Projeto" supertip="Cria uma estrutura padrão de diretório de projeto no endereço especificado." onAction="criar_diretorio_padrao" imageMso="BibliographyGallery"/>

						<button id="acessar_estrutura_padrao" label="Abrir Diretório Padrão para Customização" supertip="Abre o diretório padrão para customização." onAction="abrir_diretorio_padrao" imageMso="BibliographyGallery"/>

						<menuSeparator id="sep_operacoes_10" title="Operações com Arquivos"/>

						<button id="listar_diretorio" label="Listar arquivos de um diretório específico" supertip="Listar os nomes dos arquivos contidos em diretório específico." onAction="listar_diretorio" imageMso="ExportSavedExports"/>

						<button id="listar_diretorio_linkados" label="Listar arquivos de um diretório com links" supertip="Listar os nomes dos arquivos contidos em diretório específico e insere links." onAction="listar_diretorio_linkados" imageMso="AccessRelinkLists"/>
					
						<menuSeparator id="sep_operacoes_12" />
						
						<button	id="compilar_arquivos" label="Compilar planilhas de vários arquivos" supertip="Consolida todas as planilhas dos arquivos abertos em um novo arquivo." onAction="compilar_arquivos" imageMso="DatabaseRelationships" />

					</menu>

					<menu id="planilhas" label="Planilhas" size="normal" imageMso="QueryBuilder">

						<menuSeparator id="sep_operacoes_13" title="Operações com Planilhas"/>

						<button id="listar_planilhas" label="Listar planilhas ativas" supertip="Lista o nome de todas as planilhas ativas deste arquivo." onAction="listar_planilhas" imageMso="FileSaveAsExcelXlsx"/>
						
						<button id="compilar_planilhas" label="Compilar planilhas ativas deste arquivo" supertip="Empilha todos os registros das planilhas abertas em uma única planilha." onAction="compilar_planilhas" 									imageMso="ConvertTableToText" />
						
						<menuSeparator id="sep_operacoes_14" />

						<button id="esconder_planilha" label="Esconder planilha via código" supertip="Esconde a planilha atual. Ela não será excluída e não ficará oculta." onAction="ocultar_planilha" imageMso="SheetProtect"/>

						<button id="reexibir_planilhas_escondidas" label="Reexibir planilhas ocultas via código" supertip="Reexibir colunas ocultas via código." onAction="reexibir_planilhas_ocultas" imageMso="SheetProtect"/>
						
						<menuSeparator id="sep_operacoes_15" />

						<button	id="ocultar_todas_planilhas" label="Ocultar todas as planilhas" supertip="Oculta todas as planilhas exceto a ativa" onAction="ocultar_planilhas" imageMso="ReviewShowOrHideMarkup" />

						<button	id="reexibir_todas_planilhas" label="Reexibir todas as planilhas ocultas" supertip="Reexibir todas as planilhas ocultas" onAction="reexibir_planilhas" imageMso="ReviewShowOrHideMarkup" />	

						<menuSeparator id="sep_operacoes_98" />

						<button	id="remover_todas_as_formatacoes" label="Remover toda a formatação da planilha" supertip="Mantendo o conteúdo, remove todas as formatações da planilha atual." onAction="remover_todas_as_formatacoes" imageMso="CurrentEraser" />	

					</menu>

					<separator id="sep_operacoes_16" />

					<!-- DEFINIÇOES PARA FORMATAÇÃO DOS NÚMEROS E SALDOS-->
					<splitButton id="formatacoes" size="large">

						<button id="numeros_sem_virgulas" label="Formato Padrão de DFs" imageMso="AccountingFormat" supertip="Sem casas decimais, sem símbolos e com () pra negativos." onAction="formatarNumeros" />
						
						<menu id="lista_formatacoes">
							
							<button id="numeros_perc" label="Padrão em % Percentual" imageMso="AccountingFormat" supertip="Sem casas decimais, com símbolo de Percentual e com () pra negativos." onAction="formatarNumeros_perc" />

							<button id="numero_reais" label="Padrão de DFs em R$" imageMso="AccountingFormat" supertip="Sem casas decimais, com símbolo de Real e com () pra negativos." onAction="formatarNumeros_real" />

							<button id="numeros_dolar" label="Padrão de DFs em US$" imageMso="AccountingFormat" supertip="Sem casas decimais, com símbolo de Dólar e com () pra negativos." onAction="formatarNumeros_dolar" />

							<button id="numeros_euro" label="Padrão de DFs em EURO" imageMso="AccountingFormat" supertip="Sem casas decimais, com símbolo de Euro e com () pra negativos." onAction="formatarNumeros_euro" />

						</menu>

					</splitButton>

				<!-- DEFINIÇOES PARA TIE IN -->
					<splitButton id="tieins_inserir" size="large">

						<button id="inserirtiein" label="Composição Numérica" imageMso="PivotTableLayoutGrandTotals" supertip="Inserir tie in" onAction="InserirTieIn" />

						<menu id="lista_tieins">
							
						<button id="inserirtiein_texto" label="Composição Apenas Texto" imageMso="PivotTableLayoutSubtotals" supertip="Inserir tie in" onAction="InserirTieIn_Texto" />

						</menu>

					</splitButton>

        </group> 

        <group id="Copyright" label="Autoria" autoScale="true" centerVertically="true">

					<button id="autoria" label="Direitos Autorais" size="large" imageMso="HappyFace" supertip="Informações o responsável pelo desenvolvimento do Facilitador de Auditoria." onAction="mostrar_autoria"/>

					<button id="ajuda" label="Acessar Suporte" size="large" imageMso="ARMPreviewButton" supertip="Acessa a página com informações sobre a barra de ferramentas." onAction="mostrar_ajuda"/>

				</group>
     
